<link rel="stylesheet" href="vendor/css/bootstrap-3.3.7.css"/>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"/>
<!-- Local CSS -->
<link rel="stylesheet" href="css/main.css"/>
